{
  "name": "ionic-module-template",
  "version": "0.0.13",
  "description": "A template for creating reusable module for Angular 2 and Ionic 2",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": "dist",
  "scripts": {
    "ngc": "ngc",
    "build": "rm -rf tmp aot dist && gulp && npm run ngc && rm -rf dist/src && mv dist/tmp/** dist && rm -rf dist/tmp && rm -rf tmp aot",
    "publishPackage": "npm run build && npm publish",
    "test": "karma start ./test-config/karma.conf.js",
    "test-coverage": "karma start ./test-config/karma.conf.js --coverage"
  },
